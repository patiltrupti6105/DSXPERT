{
  "name": "dsxpert",
  "displayName": "DSXpert",
  "description": "AI-powered data structure optimization for VS Code",
  "version": "1.0.0",
  "publisher": "patiltrupti6105",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:dsxpert.chatView",
    "onCommand:dsxpert.optimizeCode"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dsxpert.optimizeCode",
        "title": "Transform Data Structure"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "dsxpert.chatView",
          "name": "DSXpert Chat"
        }
      ]
    }
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.0",
    "dotenv": "^16.3.1",
    "prettier": "^3.2.5",
      "child_process": "^1.0.2",
      "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.5",
    "@types/vscode": "^1.86.0",
     "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
     "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "vscode-test"
  }
}